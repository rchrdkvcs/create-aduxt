    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/session": "^7.5.1",
    "@tuyau/core": "^0.4.2",
    "@vinejs/vine": "^3.0.1",
